page{background:#f4f4f4; padding-bottom: 100rpx;}

/* m_xbanner */
.m_xbanner image{display:block;width:100%;}

/* m_xour */
.m_xour{position:absolute;top:22rpx;right:30rpx;width:40rpx;}
.m_xour image{width:100%;}

/* m_xwraps */
.m_xwraps{position:absolute;width:90.4%;left:4.8%;top:224rpx;border:1px solid #efeeef;background:#fff;bottom:0;overflow:hidden;}
.m_xwraps .title{border-bottom:2px solid #e6e6e6;padding:18rpx 15rpx 0 36rpx;overflow:hidden;height:102rpx;}
/* .m_xwraps .title image{} */
.m_xwraps .mins{margin-right:80rpx;}
.m_xwraps .mins text{display:block;font-size:28rpx;color:#898989;line-height:34rpx;padding-bottom:8rpx;}
.m_xwraps .mins text.top{font-weight:bold;color:#585858;}

/* m_xtabs */
.m_xtabs{border-bottom:1px solid #efeeef;overflow:hidden;padding-left:10%;}
.m_xtabs .sort{float:left;width:20%;height:72rpx;line-height:72rpx;text-align:center;border-bottom:6rpx solid #fff;color:#565656;font-size:24rpx;}
 .m_xtabs .sort:nth-child(2){margin:0 14%;} 
.m_xtabs .sort.on{border-bottom:6rpx solid #fc6466;color:#fc6466;}

/* m_xproduct */
.m_xproduct{position:absolute;left:0;width:100%;bottom:0;top:204rpx;overflow:hidden;display:none;}
.m_xproduct.top{padding-top:15rpx;}
.m_xproduct.z_show{display:block;}
.m_xproduct .navs{width:23%;position:absolute;background:#f0f2f5;left:0;top:0;bottom:93rpx;} 
.m_xproduct .navs .our{height:80rpx;line-height:80rpx;text-align:center;font-size:24rpx;color:#585858;}
.m_xproduct .navs .our.on{background:#fff;}
.m_xproduct .wares{position:absolute;left:23%;top:0;bottom:93rpx;overflow:hidden;width:77%;}
.m_xproduct .shops{float:left;width:50%;border-bottom:1px solid #efeeef;height:395rpx;overflow:hidden;box-sizing:border-box;}
.m_xproduct .shops:nth-child(2n+1){border-right:1px solid #efeeef;}
.m_xproduct .shops image.pics{display:block;margin:0 auto;}
.m_xproduct .state{margin:10rpx 0 0 7%;width:77%;font-size:24rpx; height: 104rpx; color:#585858;line-height:34rpx; overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.m_xproduct .coins{overflow:hidden;padding:6rpx 8% 0;}
.m_xproduct .coins .pay{float:left;font-size:24rpx;color:#fc6466;line-height:40rpx;}
.m_xproduct .nums{float:right;line-height:38rpx;}
.m_xproduct .nums image{display:inline-block;width:48rpx;vertical-align:middle;margin-top:-3px;cursor:pointer;}
.m_xproduct .nums text{display:inline-block;font-size:24rpx;color:#000;width:50rpx;text-align:center;}
.m_xproduct .nums .no{display:none;}
.m_xproduct .details{padding:0 0 0 34rpx;font-size:24rpx;color:#585858;line-height:50rpx;overflow:hidden;}
.m_xproduct .details image{float:right;width:35rpx;margin:10rpx 30rpx 0 0;}

/* m_xsettle */
.m_xsettle{position:fixed;width:100%;bottom:0;border:1px solid #efeeef;background:#fff;border-bottom:0;}
.m_xsettle .hide{display:none;}
.m_xsettle .show{display:block;}
.m_xsettle .all{overflow:hidden;padding:10rpx 14rpx;border-bottom:1px solid #efeeef;}
.m_xsettle .dels{float:right;font-size:20rpx;color:#585858;line-height:40rpx;}
.m_xsettle .dels image{display:inline-block;width:30rpx;vertical-align:middle;margin:-3rpx 7rpx 0 0;}
.m_xsettle .sele{display:inline-block; width: 300rpx; font-size:20rpx;color:#585858;line-height:40rpx;}
.m_xsettle .sele image{display:inline-block;width:30rpx;vertical-align:middle;margin:-3rpx 10rpx 0 0;}
.m_xsettle  .shop{border-bottom:1px solid #efeeef;overflow:hidden;padding:22rpx 14rpx 30rpx;}
.m_xsettle  .nums{float:right;line-height:40rpx;margin-top:14rpx;}
.m_xsettle  .nums image{display:inline-block;width:40rpx;vertical-align:middle;margin-top:-3px;cursor:pointer;}
.m_xsettle  .nums text{display:inline-block;font-size:24rpx;color:#000;padding:0 5rpx;width:50rpx;text-align:center;}
.m_xsettle .cont{float:left;overflow:hidden;}
.m_xsettle .cont image{float:left;}
.m_xsettle .cont image.sele{width:30rpx;margin:16rpx 35rpx 0 0;}
.m_xsettle .cont image.demo{width:94rpx;height:48rpx;margin-top:10rpx;}
.m_xsettle .cont .pay{margin-left:180rpx;}
.m_xsettle .cont text{display:block;font-size:16rpx;color:#585858;line-height:26rpx; overflow: hidden;width: 380rpx;height: 26rpx;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
}
.m_xsettle .cont text.red{color:#fc6466;font-size:22rpx;padding-top:6rpx;}
.m_xsettle .settle{overflow:hidden;}
.m_xsettle .settle .btn{float:right;width:26%;height:92rpx;line-height:92rpx;text-align:center;background:#fc6466;color:#fff;font-size:28rpx;border:none;border-radius:0;}
.m_xsettle .car{position:relative;width:76rpx;height:76rpx;margin:12rpx 16rpx 0;float:left;}
.m_xsettle .car image{display:block;width:100%;cursor:pointer;}
.m_xsettle .car text{position:absolute;width:32rpx;line-height:32rpx;height:32rpx;text-align:center;overflow:hidden;background:#ff9596;border-radius:50%;font-size:20rpx;color:#fff;top:-6rpx;right:-10rpx;}
.m_xsettle .pice{float:left;font-size:32rpx;color:#fc6466;padding:46rpx 0 0 30rpx;}

