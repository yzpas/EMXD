.m_xsize .sort{overflow:hidden;}
.m_xsize .our{float:left;background:#f5f4f5;text-align:center;height:48rpx;line-height:48rpx;color:#262626;font-size:20rpx;margin:0 24rpx 24rpx 0;border-radius:24rpx; padding: 0 20rpx; min-width: 116rpx;}
.m_xsize .current{background:#fc6466;color:#fff; }
