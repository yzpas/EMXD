  <!--main  -->
  <view class='store_main'>
    <view class='store_main_top'>
      <view class='store_money'>
        <view class='money_text'>可提现(元)</view>
        <view class='money_number'>{{shopManage.allow_withdraw}}</view>
      </view>
      <view class='deposit_btn' bindtap='deposit' wx:if="{{deposit_btn}}">提现</view>
      <view class='deposit_btns' bindtap='depositTo' wx:else>提现</view>
    </view>
    <view class='store_main_bottom'>
      <view class='store_main_bottom_li'>
        <view class='li_title'>总收入</view>
        <view class='li_main'>{{shopManage.total_income}}</view>
      </view>
      <view class='store_main_bottom_li'>
        <view class='li_title'>交易中</view>
        <view class='li_main'>{{shopManage.in_transaction}}</view>
      </view>
      <view class='store_main_bottom_li'>
        <view class='li_title'>结算中</view>
        <view class='li_main'>{{shopManage.wait_settle_accounts}}</view>
      </view>
      <view class='store_main_bottom_li'>
        <view class='li_title'>已提现</view>
        <view class='li_main'>{{shopManage.total_withdraw}}</view>
      </view>
    </view>
  </view>
  <!--入口  -->
  <view class='entrance'>
      <view class='entrance_li' bindtap='commodity'>
        <image src='/pages/images/y-icon2.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>添加商品</view>
      </view>
      <view class='entrance_li' bindtap='commodityManage'>
        <image src='/pages/images/y-icon3.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text' >商品管理</view>
      </view>
      <view class='entrance_li' bindtap='management'>
        <image src='/pages/images/y-icon4.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text' >店铺设置</view>
      </view>
      <view class='entrance_li' bindtap='sellorderMan'>
        <image src='/pages/images/y-icon5.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>订单管理</view>
      </view>
      <view class='entrance_li' bindtap='shopaddClassify'>
        <image src='/pages/images/y-icon6.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>分类管理</view>
      </view>
      <view class='entrance_li' bindtap='winDeposit'>
        <image src='/pages/images/y-icon7.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>资金明细</view>
      </view>
      <view class='entrance_li' bindtap='visitor'>
        <image src='/pages/images/y-icon9.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>最近访客</view>
      </view>
      <view class='entrance_li' bindtap='shopView'>
        <image src='/pages/images/y-icon8.png' mode='aspectFit' class='entrance_icon'></image>
        <view class='entrance_text'>店铺首页</view>
      </view>
  </view>

