/* pages/user/store/commodityManage.wxss */
.manage_tab{
  width: 100%;
  height: 58rpx;
  background-color: #ffffff;
  border-bottom: 1px solid #efeeef;
}
.tabli{
  width: 33.33%;
  float: left;
  text-align: center;
  font-size: 20rpx;
  color: #6d6d6d;
  line-height: 54rpx;
}
.select{
  color: #fd6466;
  border-bottom: 5rpx solid #fd6466;
}
.manage_main,.tab_main{
  width: 100%;
  background-color: #fff;
}
.tab_main{
  display: none;
}
.displayB{
  display: block;
}
.tab_list{
  width: 100%;
  height: 254rpx;
  border-bottom: 1px solid #efeeef;
}
.list_top{
  width: 100%;
  height: 159rpx;
  border-bottom: 1px solid #efeeef;
  padding: 20rpx 45rpx 0 17rpx;
  box-sizing: border-box;
}
.list_icon{
  width: 112rpx;
  height: 98rpx;
  border: 1px solid #efeeef;
  margin-right: 20rpx;
  display: block;
  float: left;
}
.list_text{
  float: left;
}
.text_title{
  font-size: 16rpx;
  color: #585858;
  margin-bottom: 20rpx;
}
.text_money{
  font-size: 18rpx;
  color: #585858;
  
}
.text_money text{
  font-size: 22rpx;
  color: #fd6466;
}
.list_icon1{
  width: 27rpx;
  height: 27rpx;
  display: block;
  float: right;
  padding: 48rpx 0 60rpx 60rpx;

}
.list_bom{
  padding: 15rpx 50rpx 0;
  box-sizing: border-box;
  height: 93rpx;
}
.putaway_box{
  width: 130rpx;
  /* height: 52rpx; */
  border: 1px solid #efeeef;
  padding: 6rpx 5rpx;
  box-sizing: border-box;
  overflow: hidden;
  float: left;
}
.putaway{
  width: 50%;
  float: left;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 22rpx;
  color: #acacac;
}
.colerSelect{
  background-color: #fd6466;
  color: #fff;
}
.compile{
  width: 130rpx;
  height: 52rpx;
  border: 1px solid #efeeef;
  float: right;
  text-align: center;
  line-height: 52rpx;
  font-size: 22rpx;
  color: #acacac;
}
.manage_bom{
  width: 100%;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}
.manage_bom_one{
  width: 100%;
  height: 91rpx;
  border-bottom: 1px solid #efeeef;
  padding: 0 29rpx 0 22rpx;
  line-height: 91rpx;
  box-sizing: border-box;
}
.list_icon2{
  width: 27rpx;
  height: 27rpx;
  display: block;
  float: left;
  padding: 33rpx 16rpx 33rpx 0;
}
.one_text{
  font-size: 28rpx;
  color: #a5a5a5;
}
.one_rf{
  overflow: hidden;
  height: 91rpx;
}
.one_list{
  width: 40rpx;
  /* overflow: hidden; */
  float: right;
  height: 61rpx;
  margin-top: 17rpx;
}
.one_list:nth-child(2){
  margin-right: 50rpx;
}
.list_icon3{
  width: 38rpx;
  height: 33rpx;
  float: left;
}
.one_list_text{
  font-size: 16rpx;
  color: #a5a5a5;
  /* height: 30rpx; */
  text-align: center;
  line-height: 40rpx;
}
.manage_bom_to{
  overflow: hidden;
  width: 100%;
  padding: 18rpx 24rpx;
  box-sizing: border-box;
}
.manage_bom_to_btn{
  width: 334rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border: 1px solid #efeeef;
  font-size: 28rpx;
}
.manage_bom_to_btn:nth-child(1){
  background-color: #fd6466;
  color: #fff;
}
.manage_bom_to_btn:nth-child(2){
  background-color: #f4f3f4;
  color: #585858;
  
}